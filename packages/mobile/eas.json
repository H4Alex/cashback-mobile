{
  "cli": {
    "version": ">= 13.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "20.18.0"
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_URL": "http://localhost:4000",
        "APP_ENV": "development"
      }
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "env": {
        "API_URL": "https://api-staging.h4cashback.com",
        "APP_ENV": "preview"
      }
    },
    "production": {
      "extends": "base",
      "autoIncrement": true,
      "env": {
        "API_URL": "https://api.h4cashback.com",
        "APP_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
